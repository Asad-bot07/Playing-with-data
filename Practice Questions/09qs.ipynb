   "source": [
    "#### 9. Extract all even numbers from an array using boolean indexing."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "0724a34f",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "f66b0d64",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original array :  [[51 51 16 97 25  9 22  4 24 75 78 18 28 74  1]]\n",
      "Even array :  [16 22  4 24 78 18 28 74]\n"
     ]
    }
   ],
   "source": [
    "array = np.random.randint(1, 100 , (1, 15))\n",
    "print(\"Original array : \", array, end=\"\\n\")\n",
    "evens = array[array % 2 == 0]\n",
    "print(\"Even array : \", evens)"
   ]
  }
